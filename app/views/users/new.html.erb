<h1>新規登録</h1>
<%= form_with model: @user, url: user_path, local: true do |f| %>
<div class = "field">
	<%= f.label :email %>
	<%= f.text_field :email%>
</div>
<div class = "field">
	<%= f.label :password %>
	<%= f.password_field :password %>
</div>
<div class = "field">
	<%= f.label :password_confirmeation %>
	<%= f.password_field :password_confirmeation %>
</div>
<div class = "acition">
	<%= f.submit "新規登録" %>
</div>
<%= link_to "ログインはこちら", login_path %>
<% end %>
